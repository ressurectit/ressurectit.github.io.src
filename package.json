{
    "name": "ressurectit.github.io",
    "version": "1.0.0",
    "description": "Ressurect IT github pages",
    "scripts": {
        "build": "webpack --env aot --env dll --env css --env html --config webpack.config.js",
        "build:aot": "npm run ngc -- -p tsconfig.json",
        "build:min": "webpack --env aot --env css --env html --mode=production --config webpack.config.js",
        "build:prod": "node differentialBuild.js",
        "build:gulp": "gulp build",
        "build:debug": "npm run build -- --env debug",
        "build:min:debug": "npm run build:min -- --env debug",
        "prebuild": "npm run check:dev:build && npm run version:build",
        "prebuild:aot": "npm run prebuild:min",
        "prebuild:min": "npm run clean && npm run version:build",
        "prebuild:debug": "npm run prebuild",
        "prebuild:min:debug": "npm run prebuild:min",
        "build:prepare:dev": "webpack --config webpack.config.dev.js",
        "check:dev:build": "node ifMissingDevDeps.js && (npm run clean && npm run build:prepare:dev) || echo dependecies.js is present",
        "check:dev:start": "node ifMissingDevDeps.js && (npm run build) || echo dependecies.js is present",
        "clean": "rimraf wwwroot",
        "clean:deps": "rimraf package-lock.json node_modules || echo 'node_modules removed'",
        "clean:all": "npm run clean && npm run clean:deps",
        "version:build": "ngv -e \"node write.version.js\"",
        "scss": "gulp compileScss",
        "tsc": "tsc",
        "ngc": "ngc",
        "ngcc": "ngcc",
        "postinstall": "rimraf node_modules/angular2-hotkeys/node_modules && npm run ngcc",
        "server": "node server.js",
        "lint": "eslint app/**/*.ts",
        "prestart": "npm run check:dev:start",
        "start": "npm run version:build && concurrently \"npm run server -- --webpack\" \"webpack serve --env aot --env dll --env css --env hmr --config webpack.config.js\""
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "author": "kukjevov",
    "license": "MIT",
